<!--Lista de usuarios-->

<table>
  <thead>
    <tr>
      <th>Num</th>
      <th>Nombre</th>
      <th>Correo</th>
    </tr>
  </thead>
  <tbody>
    {{#each usuarios}}
      <tr>
        <td>{{@index}}</td> <!--numero de id-->
        <td>{{Nombreusuario}}</td>
        <td>{{Correo}}</td>
        <td>
          <!--se envia a edit y se aÃ±ade identificador a url-->
          <a href="/usuario/{{_id}}/delete">Delete</a>
          <a href="/usuario/{{_id}}/edit">Update</a> 
        </td>
      </tr>

    {{/each}}
  </tbody>
</table>